package example {
	class A {
		attribute t1 : Integer;
		attribute t2 : String;
		reference b : B oppositeOf a;
	}
	
	class B {
		reference a : A oppositeOf b;
	}
	
	enumeration Choice {
		literal yes;
		literal no;
	}
}

package PrimitiveTypes {
	datatype Boolean;
	datatype Integer;
	datatype String;
}